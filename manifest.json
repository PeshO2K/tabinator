{
  "manifest_version": 3,
  "name": "Tabinator - Tab Manager",
  "description": "Auto-pins specific tabs and limits the number of open tabs, auto-closing excess tabs.",
  "version": "1.0.0",
  "permissions": ["tabs", "storage","tabGroups" ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/llama.png",
    "48": "icons/llamacool.png",
    "128": "icons/aws_logo.png"
  }
}
